    "@sendgrid/mail": "^7.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "sass": "^1.77.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}
